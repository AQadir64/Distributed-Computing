<div id="wrapper">
  <%- include ('./partials/sidebar') %>
  <div id="page-wrapper">
    <div class="container-fluid">
      <!-- Page Heading -->
      <div class="row" id="main">
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <div class="panel panel-default outer">
              <div class="panel-heading headerbar">
                <h2 class="text-center">Patients</h2>
              </div>
              <div class="panel-body text-justify">
                <div class="row">
                  <div class="col-md-8">
                    <a href="/dashboard/Add-patients" title="Add a New Player">
                      <button
                        type="button"
                        class="btn btn-primary btn-lg btn-block"
                      >
                        Add a Patient
                      </button></a
                    >
                    </div>
              <div class="col-md-4">
                  <a href="/update-patient" title="Add a New Player">
                    <button
                      type="button"
                      class="btn btn-success btn-lg"
                    >
                      Update Patients
                    </button></a
                  >

                              </div>
                </div>
                </div>
            </div>
          </div>
        </div>
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </div>
  <!-- /#page-wrapper -->
  <div class="table-wrapper"></div>
  <% if (patients.length > 0){%>
  <div class="row">
    <% patients.forEach((patient, index) => { %>
    <div class="col-md-4">
      <div class="center">
        <div class="profile">
          <div class="image">
            <div class="circle-1"></div>
            <div class="circle-2"></div>
            <img
              src="data:image/jpeg;base64,"
              width="70"
              height="70"
              alt="Jessica Potter"
            />
          </div>

          <div class="name">
            <span> <%= patient.firstname %></span>
            <span><%= patient.lastname %></span>
          </div>
          <div class="job"><%= patient.p_username %></div>
          <div class="job"><%= patient.gender %></div>
          <div class="actions">
            <button class="btn">
              <a class="ref" href="/editPatient/<%= patient.contact %>">Edit</a>
            </button>
            <button class="btn">
              <a class="ref" href="/deletePatient/<%= patient.contact %>"
                >Delete</a
              >
            </button>
          </div>
        </div>

        <div class="stats">
          <div class="box">
            <span class="value"><%= patient.contact %></span>
          </div>
          <div class="box">
            <span class="value"><%= patient.address %></span>
          </div>
          <div class="box">
            <span class="value"><%= patient.branch %></span>
          </div>
        </div>
      </div>
    </div>
    <% }) %>
  </div>
  <% } else { %>
  <p class="text-center">
    No patients found. Go <a href="/dashboard/Add-patients">here</a> to add
    patients.
  </p>
  <% } %>
</div>
